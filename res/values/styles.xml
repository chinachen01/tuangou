<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="radiobtn_warpcontent">
        <item name="android:layout_width">5dp</item>
        <item name="android:layout_height">5dp</item>
    </style>
    <style name="poi_spinner">
        <item name="android:background">@drawable/selector_spinner_back</item>
   <!--      <item name="android:textColor">@drawable/selector_spinner_text</item> -->
    </style>
	<style name="user_click_textview">
	    <item name="android:textSize">14sp</item>
	    <item name="android:paddingLeft">20dp</item>
	    <item name="android:paddingBottom">20dp</item>
	    <item name="android:paddingTop">20dp</item>
	    <item name="android:textColor">@color/txt_deep</item>
	    <item name="android:background">@drawable/selector_user_click</item>
	    <item name="android:layout_width">match_parent</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:clickable">true</item>
	    <item name="android:drawablePadding">10dp</item>
	</style>
	<style name="more_click_textview">
	    <item name="android:textSize">16sp</item>
	    <item name="android:paddingLeft">20dp</item>
	    <item name="android:paddingBottom">20dp</item>
	    <item name="android:paddingTop">20dp</item>
	    <item name="android:textColor">@color/txt_deep</item>
	    <item name="android:background">@drawable/selector_more_click</item>
	    <item name="android:layout_width">match_parent</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:clickable">true</item>
	    <item name="android:drawablePadding">10dp</item>
	</style>
</resources>
